<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape
            android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/white" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/white" />
        </shape>
    </item>
</selector>
